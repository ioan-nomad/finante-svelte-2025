  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "share_target": {
    "action": "/?share=true",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "protocol_handlers": [
    {
      "protocol": "finante",
      "url": "/?protocol=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/?import=true",
      "accept": {
        "application/pdf": [".pdf"],
        "text/csv": [".csv"],
        "application/json": [".json"]
      }
    }
  ]
